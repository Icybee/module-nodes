{
	"name": "icybee/module-nodes",
	"type": "icanboogie-module",
	"description": "Implements Nodes",
	"keywords": [
		"node"
	],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "https://olvlvl.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/Icybee/module-nodes/issues",
		"source": "https://github.com/Icybee/module-nodes"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"prefer-dist": true,
	"require": {
		"php": ">=5.6",
		"icanboogie/module-installer": "^4.0",
		"brickrouge/css-class-names": "^2.0",
		"icybee/core": "^4.0",
		"icybee/module-users": "^4.0",
		"icybee/module-sites": "^4.0",
		"icybee/module-registry": "^4.0"
	},
	"require-dev": {
		"icanboogie/bind-facets": "^0.7",
		"icanboogie/bind-activerecord": "^4.0",
		"icybee/module-modules": "^4.0",
		"icybee/module-views": "^4.0"
	},
	"suggest": {
		"icanboogie/facets": "Facets support"
	},
	"autoload": {
		"psr-4": {
			"Icybee\\Modules\\Nodes\\": "lib"
		},
		"files": [
			"helpers.php"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests/Application.php"
		]
	},
	"scripts": {
		"post-autoload-dump": "ICanBoogie\\Autoconfig\\Hooks::on_autoload_dump"
	}
}
